<div><%= flash[:error_message] %></div>

<%= form_with model: @exercise do |form| %>
  <%= form.label :name%>
  <%= form.text_field :name%>
  </br>
  <%= form.label :exercise_type%>
  <%= form.text_field :exercise_type%>
  </br>
  <%= form.label :time%>
  <%= form.number_field :time%>
  </br>
  <%= form.label :calories%>
  <%= form.number_field :calories%>
  </br>
  <%= form.label :instruction_link%>
  <%= form.text_field :instruction_link%>
  </br>
  <%= form.submit %>
<% end %>
